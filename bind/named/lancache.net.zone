$TTL 86400
@   IN  SOA     ns1.lancache.net. (
        2013042201  ;Serial
        3600        ;Refresh
        1800        ;Retry
        604800      ;Expire
        86400       ;Minimum TTL
)
; Specify our two nameservers
		IN	NS		ns1.lancache.net.
; Resolve nameserver hostnames to IP, replace with your two droplet IP addresses.
ns1		IN	A		HOST_ADDRESS

; Define hostname -> IP pairs which you wish to resolve
@		IN	A		HOST_ADDRESS
www		IN	A		HOST_ADDRESS
